local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "FELON Script HUB",
   LoadingTitle = "Loading Custom Scripts...",
   LoadingSubtitle = "by Felon HUB",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = "EnanScriptConfig",
      FileName = "MainHub"
   }
})

local Tab = Window:CreateTab("üéÆ Game Scripts", 4483362458) -- ‡πÑ‡∏≠‡∏Ñ‡∏≠‡∏ô‡∏£‡∏π‡∏õ‡∏à‡∏≠‡∏¢‡πÄ‡∏Å‡∏°

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏£‡∏±‡∏ô Script ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏õ‡∏£‡∏∞‡∏´‡∏¢‡∏±‡∏î‡∏ö‡∏£‡∏£‡∏ó‡∏±‡∏î
local function RunScript(name)
    local url = "https://raw.githubusercontent.com/Bombese/ENANSCRIPT/main/" .. name
    local success, err = pcall(function()
        loadstring(game:HttpGet(url))()
    end)
    if not success then
        Rayfield:Notify({
            Title = "Error!",
            Content = "‡πÑ‡∏°‡πà‡∏™‡∏≤‡∏°‡∏≤‡∏£‡∏ñ‡πÇ‡∏´‡∏•‡∏î‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡πÑ‡∏î‡πâ: " .. name,
            Duration = 5,
            Image = 4483362458,
        })
    end
end

-- ‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡πÄ‡∏Å‡∏°‡∏ï‡πà‡∏≤‡∏á‡πÜ
Tab:CreateButton({
   Name = "üêü Fish It",
   Callback = function() RunScript("Fish_it") end,
})

Tab:CreateButton({
   Name = "üèÉ Evade",
   Callback = function() RunScript("Evade") end,
})

Tab:CreateButton({
   Name = "üëÅÔ∏è DOORS",
   Callback = function() RunScript("Doors") end,
})

Tab:CreateButton({
   Name = "‚öîÔ∏è Solo Hunters",
   Callback = function() RunScript("Solo_hunters") end,
})

-- ‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏™‡∏£‡∏¥‡∏°‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏Ñ‡∏ô‡∏ä‡∏≠‡∏ö‡∏•‡∏∏‡∏¢
local MiscTab = Window:CreateTab("üõ†Ô∏è Extras", 4483362458)
MiscTab:CreateButton({
   Name = "Anti-AFK (‡∏Å‡∏±‡∏ô‡∏´‡∏•‡∏∏‡∏î)",
   Callback = function()
       local vu = game:GetService("VirtualUser")
       game:GetService("Players").LocalPlayer.Idled:Connect(function()
           vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
           wait(1)
           vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
       end)
       Rayfield:Notify({Title = "System", Content = "Anti-AFK Enabled!"})
   end,
})

Rayfield:Notify({
   Title = "‡∏û‡∏£‡πâ‡∏≠‡∏°‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô!",
   Content = "‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡πÄ‡∏Å‡∏°‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡∏£‡∏±‡∏ô‡πÑ‡∏î‡πâ‡πÄ‡∏•‡∏¢‡∏Ñ‡∏£‡∏±‡∏ö",
   Duration = 5,
   Image = 4483362458,
})
